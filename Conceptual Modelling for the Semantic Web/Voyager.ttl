#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( voyager:BalkanCuisine
                voyager:IberianCuisine
                voyager:ItalianFood
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( voyager:City
                voyager:Island
                voyager:Lake
                voyager:Mountain
                voyager:River
                voyager:Sea
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( voyager:Destination
                voyager:Food
                voyager:Region
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Albania
                        :Croazia
                        :Grecia
                        :Marocco
                        :Spagna
                        voyager:Italia
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Atene
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Atene
                        :Madrid
                        :Rabat
                        :Roma
                        :Tirana
                        :Zagabria
                        voyager:Barcellona
                        voyager:Dubrovnik
                        voyager:Marrakech
                        voyager:Milano
                        voyager:Salonicco
                        voyager:Valona
                      )
] .


#################################################################
#    Rules
#################################################################

:c rdf:type swrl:Variable .

:p rdf:type swrl:Variable .

:i rdf:type swrl:Variable .

:s rdf:type swrl:Variable .

:t rdf:type swrl:Variable .

:t1 rdf:type swrl:Variable .

:t2 rdf:type swrl:Variable .

:i1 rdf:type swrl:Variable .

:i2 rdf:type swrl:Variable .

:m rdf:type swrl:Variable .

:l rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Se la nazione ha una montagna e la montagna ha un lago allora la nazione ha come destinazione il lago"^^xsd:string ;
   rdfs:label "S4"^^xsd:string ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate voyager:Mountain ;
                           swrl:argument1 :m
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate voyager:HasLake ;
                                      swrl:argument1 :m ;
                                      swrl:argument2 :l
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate voyager:HasMountain ;
                                                 swrl:argument1 :s ;
                                                 swrl:argument2 :m
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate voyager:State ;
                                                            swrl:argument1 :s
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:ClassAtom ;
                                                                       swrl:classPredicate voyager:Lake ;
                                                                       swrl:argument1 :l
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate voyager:HasDestination ;
                           swrl:argument1 :s ;
                           swrl:argument2 :l
                         ] ;
               rdf:rest rdf:nil
             ]
